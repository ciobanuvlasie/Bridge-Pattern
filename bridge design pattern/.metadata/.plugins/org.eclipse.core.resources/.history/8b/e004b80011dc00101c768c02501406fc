package abstraction;

public class OldTV extends TV {

    @Override
    public void turnOn() {
        isOn = true;
        System.out.println("OldTV is ON");
    }

    @Override
    public void turnOff() {
        isOn = false;
        System.out.println("OldTV is OFF");
    }

    @Override
    public void setChannel(int channel) {
        this.channel = channel;
        System.out.println("OldTV channel set to " + channel);
    }

    @Override
    public void setVolume(int volume) {
        this.volume = volume;
        System.out.println("OldTV volume set to " + volume);
    }
}
